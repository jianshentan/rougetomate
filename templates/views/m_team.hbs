{{! default }}
<div class="rt-content rt-team">
  <!--<span class="rt-content-overlay-back-button"><span class="glyphicon glyphicon-chevron-left"></span>back</span>-->
  <h1 style="margin:6px 0px;text-transform:uppercase;font-weight:800;">Team</h2>
  <hr class="rt-content-overlay-header-line">
  <br>
  {{#each teams}}
  <!-- Thumbnail -->
  <div class="rt-team-cell">
    <img src="{{ thumbnail.url }}" >
    <div class="h3"><strong>
      {{ firstName }} {{ lastName }}, {{ position }}
    </strong></div>
    <div class="">
      {{ biography }}
    </div>
  </div>
  {{/each}}
  <br>
  <br>
</div>